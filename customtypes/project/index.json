{
  "format": "custom",
  "id": "project",
  "label": "Project",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "project_main_image": {
        "type": "Image",
        "config": {
          "label": "Project Main Image",
          "constraint": {},
          "thumbnails": []
        }
      },
      "project_title": {
        "type": "StructuredText",
        "config": {
          "label": "Project Title",
          "placeholder": "Titre du projet",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink"
        }
      },
      "subtitle": {
        "type": "Text",
        "config": {
          "label": "Subtitle",
          "placeholder": ""
        }
      },
      "client_logo": {
        "type": "Image",
        "config": {
          "label": "Client Logo",
          "constraint": {},
          "thumbnails": []
        }
      },
      "client_name": {
        "type": "Text",
        "config": {
          "label": "Client Name",
          "placeholder": "Nom du client"
        }
      },
      "quote": {
        "type": "StructuredText",
        "config": {
          "label": "Quote",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,strong,em"
        }
      },
      "organization": {
        "type": "Text",
        "config": {
          "label": "Organization",
          "placeholder": ""
        }
      },
      "date": {
        "type": "Date",
        "config": {
          "label": "Date",
          "placeholder": "Date"
        }
      },
      "category": {
        "type": "Select",
        "config": {
          "label": "Category",
          "placeholder": "Catégorie",
          "options": ["Audio", "Advertise", "Digital"]
        }
      },
      "medias": {
        "type": "Group",
        "config": {
          "label": "Medias",
          "repeat": true,
          "fields": {
            "image": {
              "type": "Image",
              "config": {
                "label": "Image",
                "constraint": {},
                "thumbnails": []
              }
            },
            "video": {
              "type": "Link",
              "config": {
                "label": "Vidéo",
                "placeholder": "",
                "select": "media",
                "allowText": true
              }
            },
            "embed": {
              "type": "Embed",
              "config": {
                "label": "Embed",
                "placeholder": ""
              }
            }
          }
        }
      },
      "testimonials": {
        "type": "Group",
        "config": {
          "label": "Testimonials",
          "repeat": true,
          "fields": {
            "name": {
              "type": "Text",
              "config": {
                "label": "name",
                "placeholder": "Full name of the person giving the testimonial"
              }
            },
            "quote": {
              "type": "StructuredText",
              "config": {
                "label": "quote",
                "placeholder": "Testimonial text",
                "allowTargetBlank": true,
                "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em"
              }
            },
            "photo": {
              "type": "Image",
              "config": {
                "label": "photo",
                "constraint": {},
                "thumbnails": []
              }
            }
          }
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {}
        }
      }
    }
  }
}
