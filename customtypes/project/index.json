{
  "format": "custom",
  "id": "project",
  "label": "Project",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "project_title": {
        "type": "StructuredText",
        "config": {
          "label": "Project Title",
          "placeholder": "Titre du projet",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,embed,list-item,o-list-item,rtl"
        }
      },
      "client": {
        "type": "Text",
        "config": {
          "label": "Client",
          "placeholder": "Nom du client"
        }
      },
      "project_main_image": {
        "type": "Image",
        "config": {
          "label": "Project Main Image",
          "constraint": {},
          "thumbnails": []
        }
      },
      "date": {
        "type": "Date",
        "config": {
          "label": "Date",
          "placeholder": "Date"
        }
      },
      "category": {
        "type": "Select",
        "config": {
          "label": "Category",
          "placeholder": "Cat√©gorie",
          "options": ["1", "2"]
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "video_embed": {
              "type": "SharedSlice"
            },
            "project_showcase": {
              "type": "SharedSlice"
            },
            "image_gallery": {
              "type": "SharedSlice"
            },
            "text_section": {
              "type": "SharedSlice"
            }
          }
        }
      }
    }
  }
}
